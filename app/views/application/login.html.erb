<div class="login-block center-block">
<%= form_tag checkLogin_path do %>
	<div class="form-group">
	 	 <%= label_tag :nickname %><br />
   		 <%= text_field_tag :nickname, params[:nickname], class: 'form-control' %>
  	</div>
  <div class="form-group">
    <%= label_tag :password %><br />
    <%= password_field_tag :password, nil, class: 'form-control' %>
  </div>
  <%= submit_tag "Log in", class: 'btn btn-default' %>
<% end %>
</div>